   "source": [
    "import numpy as np\n",
    "\n",
    "# Create a 2x3 array\n",
    "array = np.array([[1, 2, 3], [4, 5, 6]], order='C')\n",
    "print(array)\n",
    "\n",
    "\"\"\"\n",
    "Output:\n",
    "[[1 2 3]\n",
    " [4 5 6]]\n",
    "\"\"\"\n",
    "\n",
    "# Ravel with C-order (row-major)\n",
    "ravel_c = array.ravel(order='K')\n",
    "print(ravel_c)  # Output: [1, 2, 3, 4, 5, 6]"
   ]
  }
