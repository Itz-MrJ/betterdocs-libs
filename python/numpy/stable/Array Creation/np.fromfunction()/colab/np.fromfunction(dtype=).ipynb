    "import numpy as np\n",
    "\n",
    "# 1x5 array where each entry is i + j (row + column index)\n",
    "fromfunction_array = np.fromfunction(function=lambda i, j: i + j, shape=(1, 5), dtype=np.int32)\n",
    "print(fromfunction_array)\n",
    "'''\n",
    "Output:\n",
    "[[0 1 2 3 4]]\n",
    "'''"
