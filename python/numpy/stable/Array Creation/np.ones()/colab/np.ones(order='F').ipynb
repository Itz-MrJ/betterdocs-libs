    "import numpy as np\n",
    "\n",
    "# Create a 2x3 array\n",
    "array = np.ones([2, 3], order='F')\n",
    "print(array)\n",
    "\n",
    "\"\"\"\n",
    "Output:\n",
    "[[1. 1. 1.]\n",
    " [1. 1. 1.]]\n",
    "\"\"\"\n",
    "\n",
    "# Ravel with K-order\n",
    "ravel_c = array.ravel(order='K')\n",
    "print(ravel_c)  # Output: [1. 1. 1. 1. 1. 1.]"
   ]
  }
