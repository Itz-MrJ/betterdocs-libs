    "import numpy as np\n",
    "\n",
    "# Create a zeros_like 2x2 array\n",
    "a = np.array([[1, 2], [2, 5]])\n",
    "array = np.zeros_like(a=a, order='A')\n",
    "print(array)\n",
    "\n",
    "\"\"\"\n",
    "Output:\n",
    "[[0 0]\n",
    " [0 0]]\n",
    "\"\"\"\n",
    "\n",
    "# Ravel with K-order\n",
    "ravel_c = array.ravel(order='K')\n",
    "print(ravel_c)  # Output: [0 0 0 0]"
   ]
  }
